  },
  "electrochemical_models": {
    "Butler_Volmer": {
      "equation": "i = i0 * [exp(αnFη/RT) - exp(-(1-α)nFη/RT)]",
      "parameters": {
        "i0": "Exchange current density",
        "α": "Transfer coefficient (typically 0.5)",
        "n": "Number of electrons",
        "F": "Faraday constant (96485 C/mol)",
        "η": "Overpotential"
      }
    },
    "Arrhenius_Conductivity": {
      "equation": "σ = (σ0/T) * exp(-Ea/kT)",
      "parameters": {
        "σ0": "Pre-exponential factor",
        "Ea": "Activation energy",
        "k": "Boltzmann constant"
      }
    }
  },
