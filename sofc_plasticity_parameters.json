  "metadata": {
    "description": "SOFC Materials Plasticity Model Parameters Database",
    "version": "1.0",
    "date_created": "2025-10-03",
    "models": ["Johnson-Cook", "Ramberg-Osgood", "Power-Law Hardening", "Chaboche"],
    "units": {
      "stress": "MPa",
      "strain": "dimensionless",
      "strain_rate": "1/s",
      "temperature": "K"
    }
  },
  "plasticity_models": {
    "Johnson_Cook": {
      "description": "Temperature and strain-rate dependent plasticity",
      "equation": "σ = (A + B*ε_p^n) * (1 + C*ln(ε_dot/ε_dot_0)) * (1 - T_h^m)",
      "parameters": {
        "A": "Initial yield stress (MPa)",
        "B": "Hardening modulus (MPa)",
        "n": "Hardening exponent",
        "C": "Strain rate sensitivity",
        "m": "Thermal softening exponent",
        "T_h": "Homologous temperature = (T-T_room)/(T_melt-T_room)"
      }
    },
    "Ramberg_Osgood": {
      "description": "Power-law stress-strain relationship",
      "equation": "ε = σ/E + K*(σ/σ_y)^n",
      "parameters": {
        "K": "Strength coefficient",
        "n": "Hardening exponent",
        "σ_y": "Yield stress"
      }
    },
    "Chaboche": {
      "description": "Kinematic/isotropic hardening for cyclic loading",
      "equation": "Combined isotropic-kinematic hardening",
      "parameters": {
        "C": "Kinematic hardening modulus",
        "γ": "Kinematic hardening rate",
        "Q": "Isotropic hardening asymptotic value",
        "b": "Isotropic hardening rate"
      }
    }
  },
