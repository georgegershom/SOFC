    "strain_rate": "1e6 - 1e8 s⁻¹",
    "system_size": "50x50x50 unit cells (125,000 atoms)",
    "potential": "EAM (Al-Mishin 2003)",
    "integration": "Velocity Verlet, 2 fs timestep",
    "total_time": "200 ps per simulation"
