  "creep_models": {
    "Norton_Bailey": {
      "description": "Primary creep model for SOFC materials",
      "equation": "ε_cr = B * σ^n * t^m * exp(-Q/RT)",
      "parameters": {
        "B": "Pre-exponential factor",
        "n": "Stress exponent",
        "m": "Time exponent (typically 0.3-0.5)",
        "Q": "Activation energy"
      }
    },
    "Power_Law": {
      "description": "Steady-state creep",
      "equation": "dε/dt = A * σ^n * exp(-Q/RT)"
    },
    "Garofalo": {
      "description": "Hyperbolic sine law for wide stress range",
      "equation": "dε/dt = A * [sinh(α*σ)]^n * exp(-Q/RT)"
    }
  },
